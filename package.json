{
  "name": "@jjwesterkamp/toolbelt",
  "version": "0.1.2",
  "description": "Generic utilities for typescript projects",
  "main": "index.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:esm": "rm -rf ./esm && tsc --module ES2015   --outDir esm --listEmittedFiles",
    "build:cjs": "rm -rf ./cjs && tsc --module CommonJS --outDir cjs --listEmittedFiles",
    "test": "jest"
  },
  "author": "Jeffrey Westerkamp",
  "license": "MIT",
  "exports": {
    "./assertions": {
      "import": "./esm/assertions/assertions.js",
      "require": "./cjs/assertions/assertions.js"
    },
    "./either": {
      "import": "./esm/either/either.js",
      "require": "./cjs/either/either.js"
    },
    "./maybe": {
      "import": "./esm/maybe/maybe.js",
      "require": "./cjs/maybe/maybe.js"
    },
    "./rxjs-maybe": {
      "import": "./esm/rxjs-maybe/rxjs-maybe.js",
      "require": "./cjs/rxjs-maybe/rxjs-maybe.js"
    }
  },
  "peerDependencies": {
    "rxjs": "6.* || 7.* || 8.*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
