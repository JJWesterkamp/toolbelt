{
  "name": "@jjwesterkamp/toolbelt",
  "version": "0.0.0",
  "description": "Generic utilities for typescript projects",
  "main": "index.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:esm15": "rm -rf ./esm15 && tsc --outDir esm15 --module ES2015 --listEmittedFiles",
    "build:commonjs": "rm -rf ./commonjs && tsc --outDir commonjs --module CommonJS --listEmittedFiles",
    "test": "jest"
  },
  "author": "Jeffrey Westerkamp",
  "license": "MIT",
  "peerDependencies": {
    "rxjs": "6.* || 7.* || 8.*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
